{
    "name": "Facebook Color",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 3,
    "icons": { "16": "time.png",
               "48": "time.png",
               "128": "time.png" },
    "action": {
      "default_popup": "index.html"
       
    },
    "content_scripts": [
        {
          "matches": ["https://www.google.com/*", "https://www.google.com.vn/*" ],
          "css": ["style.css"],
          "js": ["custom.js"]
        }
      ]
      
  }